<?xml version="1.0" encoding="utf-8"?>
<lqn-model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="collab_default" xsi:noNamespaceSchemaLocation="lqn.xsd">
   <processor multiplicity="1" name="Resource1_processor" quantum="0.001" scheduling="ps" speed-factor="1">
      <task name="Resource1_task" scheduling="inf">
         <entry name="AuthorSOP_entry" type="NONE"/>
         <entry name="ManufacturingReview_entry" type="NONE"/>
         <task-activities>
            <activity bound-to-entry="AuthorSOP_entry" call-order="STOCHASTIC" host-demand-cvsq="1" host-demand-mean="600" name="AuthorSOP_activity"/>
            <activity bound-to-entry="ManufacturingReview_entry" call-order="STOCHASTIC" host-demand-cvsq="1" host-demand-mean="120" name="ManufacturingReview_activity"/>
            <reply-entry name="AuthorSOP_entry">
               <reply-activity name="AuthorSOP_activity"/>
            </reply-entry>
            <reply-entry name="ManufacturingReview_entry">
               <reply-activity name="ManufacturingReview_activity"/>
            </reply-entry>
         </task-activities>
      </task>
   </processor>
   <processor multiplicity="1" name="Resource2_processor" scheduling="fcfs" speed-factor="1">
      <task name="Resource2_task" scheduling="inf">
         <entry name="DocumentationReview_entry" type="NONE"/>
         <entry name="QualityCheck_entry" type="NONE"/>
         <task-activities>
            <activity bound-to-entry="DocumentationReview_entry" call-order="STOCHASTIC" host-demand-cvsq="1" host-demand-mean="300" name="DocumentationReview_activity"/>
            <activity bound-to-entry="QualityCheck_entry" call-order="STOCHASTIC" host-demand-cvsq="1" host-demand-mean="180" name="QualityCheck_activity"/>
            <reply-entry name="DocumentationReview_entry">
               <reply-activity name="DocumentationReview_activity"/>
            </reply-entry>
            <reply-entry name="QualityCheck_entry">
               <reply-activity name="QualityCheck_activity"/>
            </reply-entry>
         </task-activities>
      </task>
   </processor>
   <processor name="Individual project (fork-join)_processor" scheduling="inf" speed-factor="1">
      <task multiplicity="10" name="Individual project (fork-join)_task" scheduling="ref" think-time="1e-08">
         <entry name="StartEvent_entry" type="NONE"/>
         <task-activities>
            <activity bound-to-entry="StartEvent_entry" call-order="STOCHASTIC" host-demand-cvsq="1e-08" host-demand-mean="1e-08" name="StartEvent"/>
            <activity call-order="STOCHASTIC" host-demand-cvsq="1e-08" host-demand-mean="1e-08" name="AuthorSOP">
               <synch-call calls-mean="1" dest="AuthorSOP_entry"/>
            </activity>
            <activity call-order="STOCHASTIC" host-demand-cvsq="1e-08" host-demand-mean="1e-08" name="parallelGateway1"/>
            <activity call-order="STOCHASTIC" host-demand-cvsq="1e-08" host-demand-mean="1e-08" name="DocumentationReview">
               <synch-call calls-mean="1" dest="DocumentationReview_entry"/>
            </activity>
            <activity call-order="STOCHASTIC" host-demand-cvsq="1e-08" host-demand-mean="1e-08" name="ManufacturingReview">
               <synch-call calls-mean="1" dest="ManufacturingReview_entry"/>
            </activity>
            <activity call-order="STOCHASTIC" host-demand-cvsq="1e-08" host-demand-mean="1e-08" name="parallelGateway2"/>
            <activity call-order="STOCHASTIC" host-demand-cvsq="1e-08" host-demand-mean="1e-08" name="QualityCheck">
               <synch-call calls-mean="1" dest="QualityCheck_entry"/>
            </activity>
            <activity call-order="STOCHASTIC" host-demand-cvsq="1e-08" host-demand-mean="1e-08" name="EndEvent"/>
            <precedence>
               <pre>
                  <activity name="StartEvent"/>
               </pre>
               <post>
                  <activity name="AuthorSOP"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="AuthorSOP"/>
               </pre>
               <post>
                  <activity name="parallelGateway1"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="parallelGateway1"/>
               </pre>
               <post-AND>
                  <activity name="DocumentationReview"/>
                  <activity name="ManufacturingReview"/>
               </post-AND>
            </precedence>
            <precedence>
               <pre-AND>
                  <activity name="DocumentationReview"/>
                  <activity name="ManufacturingReview"/>
               </pre-AND>
               <post>
                  <activity name="parallelGateway2"/>
               </post>
            </precedence>
            <precedence>
               <pre>
                  <activity name="parallelGateway2"/>
               </pre>
               <post-LOOP end="QualityCheck"/>
            </precedence>
            <precedence>
               <pre>
                  <activity name="QualityCheck"/>
               </pre>
               <post>
                  <activity name="EndEvent"/>
               </post>
            </precedence>
         </task-activities>
      </task>
   </processor>
</lqn-model>