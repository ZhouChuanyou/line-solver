<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<lqn-model description="test_LQN_12" name="test_LQN_12" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="lqn.xsd">
