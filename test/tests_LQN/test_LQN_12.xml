            <precedence>
               <pre>
                  <activity name="AS1"/>
               </pre>
               <post>
                  <activity name="AS2"/>
               </post>
            </precedence>               
            <precedence>
               <pre>
                  <activity name="AS2"/>
               </pre>
               <post>
                  <activity name="AS3"/>
