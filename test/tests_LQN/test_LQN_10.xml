<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<lqn-model description="test_LQN_10" name="test_LQN_10" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="lqn.xsd">
    <processor name="P1" scheduling="inf">
        <task name="T1" scheduling="ref"  think-time="1000" multiplicity="100">  <!-- reference task T1 -->
            <entry name="E1" type="Graph" />
            <task-activities>
                <activity name="AS1" host-demand-mean="200.0"  bound-to-entry="E1" >
                    <synch-call calls-mean="1" dest="E2" />
                </activity>
                <activity name="AS2" host-demand-mean="2.0" >
                    <synch-call calls-mean="1" dest="E3" />
                </activity>
            <precedence>
               <pre>
                  <activity name="AS1"/>
               </pre>
               <post>
                  <activity name="AS2"/>
               </post>
            </precedence>                               
            </task-activities>
        </task>
    </processor>
    <processor name="P2" scheduling="inf" >
        <task name="T2" scheduling="fcfs" multiplicity="15">  
            <entry name="E2" type="Graph" />
            <entry name="E3" type="Graph" />
            <task-activities>
                <activity name="AS3" host-demand-mean="1.0"  bound-to-entry="E2" >
                    <synch-call calls-mean="1" dest="E4" />
                </activity>
                <activity name="AS4" host-demand-mean="10.0"  bound-to-entry="E3" />
                <reply-entry name="E2">
                    <reply-activity name="AS3"/>
                </reply-entry>
                <reply-entry name="E3">
                    <reply-activity name="AS4"/>
                </reply-entry>
            </task-activities>
        </task>
    </processor>
    <processor name="P3" scheduling="ps" quantum="0.001">
        <task name="T3" scheduling="fcfs" multiplicity="2"> 
            <entry name="E4" type="Graph" />
            <task-activities>
                <activity name="AS5" host-demand-mean="7.0"  bound-to-entry="E4" />
                <reply-entry name="E4">
                    <reply-activity name="AS5"/>
                </reply-entry>
            </task-activities>
        </task>
    </processor>
</lqn-model>