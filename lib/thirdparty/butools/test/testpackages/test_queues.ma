lambd = Total[pi0.Inverse[IdentityMatrix[Dimensions[R][[1]]]-R].F];
BuTools`Verbose = True;
Print["Test:"//OutputForm];
Print["-----"//OutputForm];
Print["{ncd, ncm} = QBDQueue[B, L, F, L0, \"ncDistr\", 11, \"ncMoms\", 5];:"//OutputForm];
